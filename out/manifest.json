{
    "name": "Runescape Toolz",
	"description": "Just a simple Runescape tool that allows for easy access to on-site search.",
    "version": "0.0.1",
	"manifest_version": 2,
	"icons" : {
        },
    "browser_action" : {
        "default_popup" : "popup.html"
    },
    "options_page" : "options.html",
    "content_scripts": [
        {
            "matches":[
                "https://services.runescape.com/*"
            ],
            "js":[
                "jquery-2.0.3.min.js",
                "contentscript.js"
            ]
        }
    ],
    "permissions":[
        "https://services.runescape.com/*"
    ],
    "web_accessible_resources":[
        "contentscript.js.map",
        "options.js.map"
    ]
}
